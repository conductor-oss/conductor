{
  "index_patterns": [ "*message*" ],
  "template": {
    "settings": {
      "refresh_interval": "1s"
    },
    "mappings": {
      "properties": {
        "created": {
          "type": "long"
        },
        "messageId": {
          "type": "keyword",
          "index": true
        },
        "payload": {
          "type": "keyword",
          "index": true
        },
        "queue": {
          "type": "keyword",
          "index": true
        }
      }
    },
    "aliases": { }
  }
}
