{
  "index_patterns": [ "*task*log*" ],
  "template": {
    "settings": {
      "refresh_interval": "1s"
    },
    "mappings": {
      "properties": {
        "createdTime": {
          "type": "long"
        },
        "log": {
          "type": "keyword",
          "index": true
        },
        "taskId": {
          "type": "keyword",
          "index": true
        }
      }
    },
    "aliases": { }
  }
}
